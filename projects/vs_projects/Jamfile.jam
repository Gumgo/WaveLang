import path ;
import vs-project-generator ;

echo "Generating VS project files" ;

# Call each project's jamfile to set up the VS project config files
build-project $(TOP)/projects/compiler ;
build-project $(TOP)/projects/console ;
build-project $(TOP)/projects/runtime ;

# Set up the VS solution config file and call the VS project generator
vs-project-generator.setup-vs-solution-config $(TOP) : wavelang ;
